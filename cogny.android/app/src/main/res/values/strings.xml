  <string name="sql_backup_user_insert" translatable="false">INSERT INTO UserInsert (userNo, partnerNo, partnerName, signProvider, email, name, hpNo, regDate) SELECT userNo, partnerNo, partnerName, signProvider, email, name, hpNo, regDate FROM User</string>
  <string name="sql_backup_user_mobile_device_insert" translatable="false">INSERT INTO UserMobileDeviceInsert (userMobileDeviceNo, mobileDeviceNo, mobileNumber) SELECT userMobileDeviceNo, mobileDeviceNo, mobileNumber FROM UserMobileDevice</string>
  <string name="sql_backup_obd_device_insert" translatable="false">INSERT INTO ObdDeviceInsert (obdDeviceNo, obdSerial, regDate) SELECT obdDeviceNo, obdSerial, regDate FROM ObdDevice</string>
  <string name="sql_backup_vehicle_insert" translatable="false">INSERT INTO VehicleInsert (vehicleNo, modelNo, fuelNo, licenseNo, modelYear, modelMonth, regDate, modelName, fuelName, manufacturerNo, manufacturerName) SELECT vehicleNo, modelNo, fuelNo, licenseNo, modelYear, modelMonth, regDate, modelName, fuelName, manufacturerNo, manufacturerName FROM Vehicle</string>
  <string name="sql_backup_drive_history_insert" translatable="false">INSERT INTO DriveHistoryInsert (driveHistoryNo, vehicleNo, obdDeviceNo, userNo, userMobileDeviceNo, startDate, startTime, startLatitude, startLongitude, startAddress, startMileage, endTime, endLatitude, endLongitude, endAddress, endMileage, driveDistance) SELECT driveHistoryNo, vehicleNo, obdDeviceNo, userNo, userMobileDeviceNo, startDate, startTime, startLatitude, startLongitude, startAddress, startMileage, endTime, endLatitude, endLongitude, endAddress, endMileage, driveDistance FROM DriveHistory</string>

  <string name="sql_restore_user_insert" translatable="false">INSERT INTO User (userNo, partnerNo, partnerName, signProvider, email, name, hpNo, regDate) SELECT userNo, partnerNo, partnerName, signProvider, email, name, hpNo, regDate FROM UserInsert</string>
  <string name="sql_restore_user_mobile_device_insert" translatable="false">INSERT INTO UserMobileDevice (userMobileDeviceNo, mobileDeviceNo, mobileNumber) SELECT userMobileDeviceNo, mobileDeviceNo, mobileNumber FROM UserMobileDeviceInsert</string>
  <string name="sql_restore_obd_device_insert" translatable="false">INSERT INTO ObdDevice (obdDeviceNo, obdSerial, regDate) SELECT obdDeviceNo, obdSerial, regDate FROM ObdDeviceInsert</string>
  <string name="sql_restore_vehicle_insert" translatable="false">INSERT INTO Vehicle (vehicleNo, modelNo, fuelNo, licenseNo, modelYear, modelMonth, regDate, modelName, fuelName, manufacturerNo, manufacturerName) SELECT vehicleNo, modelNo, fuelNo, licenseNo, modelYear, modelMonth, regDate, modelName, fuelName, manufacturerNo, manufacturerName FROM VehicleInsert</string>
  <string name="sql_restore_drive_history_insert" translatable="false">INSERT INTO DriveHistory (driveHistoryNo, vehicleNo, obdDeviceNo, userNo, userMobileDeviceNo, startDate, startTime, startLatitude, startLongitude, startAddress, startMileage, endTime, endLatitude, endLongitude, endAddress, endMileage, driveDistance) SELECT driveHistoryNo, vehicleNo, obdDeviceNo, userNo, userMobileDeviceNo, startDate, startTime, startLatitude, startLongitude, startAddress, startMileage, endTime, endLatitude, endLongitude, endAddress, endMileage, driveDistance FROM DriveHistoryInsert</string>

